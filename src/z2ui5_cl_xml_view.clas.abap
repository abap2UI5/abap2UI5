              t_prop = VALUE #( ( n = `VALUE` v = val ) ) ).
  ENDMETHOD.


  METHOD _generic.
    DATA(result2) = NEW z2ui5_cl_xml_view( ).
    result2->m_name   = name.
    result2->m_ns     = ns.
    result2->mt_prop  = t_prop.
    result2->m_parent = me.
    result2->m_root   = m_root.
    INSERT result2 INTO TABLE t_child.

    m_root->m_last = result2.
    result = result2.
  ENDMETHOD.
